.viewFramework-sidebar-full .sidebar-fold{
	background: url(images/icons.png) center 0 no-repeat;
}
.viewFramework-sidebar-mini .sidebar-fold{
	background: url(images/icons.png) center -30px no-repeat;
}
.viewFramework-sidebar-full .sidebar-title-text{
	display:inline-block;
}
.viewFramework-sidebar-mini .sidebar-title-text{
	display: none;
}

.viewFramework-sidebar {
    background-color: #2a3542;
}
.viewFramework-sidebar .sidebar-nav {
    width: 180px;
}

.viewFramework-sidebar .sidebar-nav{
	cursor: pointer;
	 color: #fff;
}

.viewFramework-sidebar .sidebar-nav .sidebar-title {
    -moz-user-select: none;
    background: #22282e none repeat scroll 0 0;
    height: 40px;
    line-height: 40px;
    position: relative;
}
.viewFramework-sidebar .sidebar-nav-fold .sidebar-title {
    background: #424f5d none repeat scroll 0 0;
    border-bottom: 1px solid #586676;
}

.viewFramework-sidebar .sidebar-nav .sidebar-title-icon {
    display: inline-block;
    margin: 0 8px 0 21px;
    transition: transform 0.12s ease 0s;
    vertical-align: middle;
}
.viewFramework-sidebar .sidebar-nav-fold  .sidebar-title-icon {
	transform: rotate(-90deg);
	-o-transform: rotate(-90deg); /* Opera浏览器 */
	-webkit-transform: rotate(-90deg); /* Webkit内核浏览器 */
	-moz-transform: rotate(-90deg); /* Firefox浏览器 */

}

.viewFramework-sidebar .sidebar-nav .sidebar-title-text {
    color: #fff;
    cursor: pointer;
    line-height: 40px;
    font-size: 0.8em;
}
.icon-arrow-down{
	width: 8px;
	background: url(images/icons.png) -11px -175px no-repeat;
}
.viewFramework-sidebar .sidebar-manage {
    height: 30px;
    position: absolute;
    right: 0;
    vertical-align: middle;
    width: 25px;
}
.icon-module-set{
	width: 8px;
	background: url(images/icons.png) center -388px no-repeat;
}


.viewFramework-sidebar .sidebar-nav ul {
    list-style: outside none none;
    margin: 0;
    overflow: hidden;
    padding: 0;
}
.viewFramework-sidebar .sidebar-trans {
    transition: all 0.12s ease 0s, all 0.12s ease 0s;
}

.viewFramework-sidebar .sidebar-nav .nav-item {
    position: relative;
}

.viewFramework-sidebar .sidebar-nav li a {
    display: block;
    height: 40px;
    line-height: 40px;
    overflow: hidden;
    width: 100%;
}

.viewFramework-sidebar-full .viewFramework-sidebar .sidebar-nav .nav-icon, .viewFramework-sidebar.sidebar-full .sidebar-nav .nav-icon {
    width: 50px;
}
.viewFramework-sidebar .sidebar-nav .nav-icon {
    color: #aeb9c2;
    float: left;
    font-size: 16px;
    text-align: center;
    width: 50px;
}
/*.icon-ecs::before {}*/
.viewFramework-sidebar .sidebar-nav .nav-icon span{
	display: block;
	width: 100%;
	height: 40px;
	background-image: url(images/subSysIcons.png);
	background-repeat: no-repeat;
}
