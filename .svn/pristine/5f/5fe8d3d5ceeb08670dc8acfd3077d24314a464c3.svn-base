/*
 * 保存、取消按钮
 ============================
 * */
.submitBox{
	margin-top: 50px;
	padding: 8px 50px 8px 8px;
	text-align: right;
}
a.commBtn:link, a.commBtn:visited{
	width: 92px;
	padding: 4px 0;
	margin-left: 15px;
	background: #FFFFFF;
	border: 1px solid #d4d4d4;
	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
	-o-box-border-radius: 3px;
	-ms-box-border-radius: 3px;
	color: #b8b8b8;
	text-align: center;
	text-decoration: none;
}
a.commBtn:hover{
	border: 1px solid #00b6f2;
	color: #00b6f2;
}
a.submitBtn:link, a.submitBtn:visited{
	background: #ffffff;
	border: 1px solid #ffffff;
	color: #FFFFFF;
}
a.submitBtn:hover{
	background: #00b6f2;
	border: 1px solid #00b6f2;
}


/*
 * 我的评价表
 * */
.assessLab{
	border-top: 3px solid #0099cb;
	border-bottom: 1px solid #dfdedc;
}
.assessLab a{
	padding: 0 10px;
	border-right: 1px solid #dedede;
}
.assessLab a.visitedLab{
	padding: 8px 10px;
	background: #0099cb;
	color: #FFFFFF;
	border: none;
	text-decoration: none;
}
.assessContent{
	margin-top: 10px;
	padding: 10px;
	border: 1px solid #80cce3;
	font-size: 0;
}
.overAssess{display: none;}
a.accessObj:link,a.accessObj:visited{
	width: 110px;
	height: 110px;
	margin:0 10px 10px 0;
	background: #fafafa;
	border: 1px solid #dedede;
	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
	-o-box-border-radius: 3px;
	-ms-box-border-radius: 3px;
	text-align: center;
}
a.accessObj:hover{
	border: 1px solid #84d5f0;
}
.accessObj img{
