.viewFramework-sidebar .sidebar-nav .nav-title {
    color: #a7b1c2;
    display: block;
    float: left;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    width: 130px;
    font-size: 0.9em;
}

.viewFramework-sidebar .sidebar-nav li a:hover {
	background: #35404d none repeat scroll 0 0;
}

.viewFramework-sidebar .sidebar-nav li.active a {
	border-left: 3px solid #00bfff;
    background: #35404d none repeat scroll 0 0;
}
.viewFramework-sidebar .sidebar-nav li.active .nav-title{
	width: 127px;
}
.viewFramework-sidebar .sidebar-nav li.active a .nav-title {
