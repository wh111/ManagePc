/*
 * 左侧二级菜单
 * */
.viewFramework-main-navbar-collapse span{
	display: block;
	width: 100%;
	height: 60px;
	background-image: url(images/icons.png);
	background-repeat: no-repeat;
}
.viewFramework-main-navbar-full .icon-collapse-left{
	background-position: center -60px;
}
.viewFramework-main-navbar-mini .icon-collapse-left{
	background-position: center -120px;
}
.viewFramework-main-navbar .main-nav-main-scene .main-nav-title {
    background: #b3d8eb none repeat scroll 0 0;
    height: 70px;
    line-height: 70px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    width: 180px;
    font-weight: bold;
    text-indent: 20px;
    color: #ffffff;
    border-bottom: 2px solid #ffffff;
    box-sizing: border-box;
}
.viewFramework-main-navbar .main-nav-list {
    bottom: 0;
    left: 0;
    overflow-x: hidden;
    overflow-y: auto;
    position: absolute;
    right: 0;
    top: 70px;
}
.viewFramework-main-navbar .main-nav-list li.active{
	border-top: 1px solid #d7dfe4;
	border-bottom: 1px solid #d7dfe4;
}
.viewFramework-main-navbar .main-nav-list li.active a
,.viewFramework-main-navbar .main-nav-list li.active a:hover {
    background-color: #FFFFFF;
    font-weight: bold;
    color: #ffffff;
}
.viewFramework-main-navbar .main-nav-list li a {
    color: #333;
    display: block;
    height: 40px;
    line-height: 40px;
    width: 180px;
    font-size: 0.8em;
}
.viewFramework-main-navbar .main-nav-list li a:hover {
	background: #f3f3f4;
}
.viewFramework-main-navbar .main-nav-list .nav-icon {
    color: #333;
    float: left;
    font-size: 16px;
    height: 40px;
    text-align: center;
    width: 30px;
}
.viewFramework-main-navbar .main-nav-list .nav-icon span{
	display: block;
	margin-top: 5px;
	width: 30px;
	height: 30px;
	background-image: url(images/icons.png);
	background-repeat: no-repeat;
	transition: transform 0.12s ease 0s;
	transform: rotate(-90deg);
}
.viewFramework-main-navbar .main-nav-list .nav-icon span.transform0{
	transform: rotate(0deg);
}
.viewFramework-main-navbar .main-nav-list .nav-title {
    float: left;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    width: 138px;
}
.icon-arrow-right{
	background-position: center -240px;
}

.viewFramework-main-navbar .main-nav-list ul ul li a {
    color: #666;
}
.viewFramework-main-navbar .main-nav-list ul ul li a .nav-title {
    text-indent: 8px;
}
.ng-hide {
    display: none !important;
}

