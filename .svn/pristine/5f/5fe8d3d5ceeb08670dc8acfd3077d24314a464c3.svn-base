/*
 *
 *  @module: 头部内容 by zyc
 *  @content: start
 * ***************************************************************************
 * */

.ambuf-console-topbar .topbar-clearfix::after {
    clear: both;
}
.ambuf-console-topbar .topbar-clearfix::before, .ambuf-console-topbar .topbar-clearfix::after {
    content: " ";
    display: table;
}
.ambuf-console-topbar {
    background: #ffffff none repeat scroll 0 0;
    clear: both;
    font-size: 12px;
    height: 60px;
    min-width: 990px;
    position: relative;
    z-index: 100;
}
.ambuf-console-topbar .topbar-wrap
,.ambuf-console-topbar .topbar-info{
    height: 100%;
}
.ambuf-console-topbar .topbar-left{
    float: left;
}
.ambuf-console-topbar .topbar-right{
    float: left;
}

.ambuf-console-topbar .topbar-yylogo{
    position: relative;
	width: 200px;
    height: 60px;
    padding: 0 1px;
    background: #ffffff none repeat scroll 0 0;
    z-index: 3;
}
.ambuf-console-topbar .topbar-yylogo .topbar-yylogo-imgBox{
	margin-top: 12px;
	margin-left: 20px;
	height: 40px;
}

/*.ambuf-console-topbar .topbar-yylogo img{*/
/*width: 100%;*/
/*}*/
.ambuf-console-topbar .topbar-yylogo a
,.ambuf-console-topbar .topbar-workbench a
,.ambuf-console-topbar .topbar-global-nav a
,.ambuf-console-topbar .topbar-zyysLogo a
,.ambuf-console-topbar .topbar-mobile-edition a{
	display: block;
	width: 100%;
	height: 100%;
	font-size: 1.2em;
	color: #addff2;
}
.ambuf-console-topbar .topbar-workbench a:hover
,.ambuf-console-topbar .topbar-global-nav a:hover
,.ambuf-console-topbar .topbar-mobile-edition a:hover{
	background: #ffffff;
}

.ambuf-console-topbar .topbar-global-nav-vsted a
,.ambuf-console-topbar .topbar-global-nav-vsted a:hover{
	background: #FFFFFF;
	color: #333;
}

.icon-phone{
	width: 20px;
	height: 30px;
	display: inline-block;
	background-image: url(images/icons.png);
	background-repeat: no-repeat;
	background-position: center -212px;
}

.ambuf-console-topbar .topbar-workbench
,.ambuf-console-topbar .topbar-global-nav
,.ambuf-console-topbar .topbar-mobile-edition{
    width: 100px;
    height: 60px;
    line-height: 60px;
    border-right: 1px solid #ffffff;
    text-align: center;

}
.ambuf-console-topbar .topbar-info{
	background: #ffffff none repeat scroll 0 0;
	position: absolute;
    right: 0;
    top: 0;
	z-index: 1;
	margin-right: 30px;
}
.ambuf-console-topbar .topbar-zyysLogo{
	width: 100px;
	height: 60px;
}
.ambuf-console-topbar .topbar-user{
	/*width: 267px;*/
	height: 60px;
}
.ambuf-console-topbar .topbar-remind{
	position: relative;
	display: inline-block;
	width: 63px;
	height: 28px;
	margin-left: 10px;
	margin-top: 11px;
}
.ambuf-console-topbar .topbar-remind-list{
	position: absolute;
	top: 40px;
	right: 268px;
	width: 310px;
    min-width: 0;
	border: medium none;
	background: #FFFFFF;
    display: none;
    float: left;
    padding: 10px;
    text-shadow: none;
    z-index: 1000;
    border-radius: 3px;
    box-shadow: 0 0 3px rgba(86,96,117,.3);
}
.ambuf-console-topbar .topbar-remind-list li{
	border-bottom: 1px solid #e7eaec;
}
.ambuf-console-topbar .topbar-remind-list li a{
	padding: 3px 20px;
    min-height: 0;
}
.ambuf-console-topbar .topbar-remind-list li.topbar-remind-more{
	display: block;
	width: 100%;
	text-align: center;
	border: none;
}
.ambuf-console-topbar .topbar-remind-list li.topbar-remind-more a{
	color: #ffffff;
}

.ambuf-console-topbar .topbar-remind .topbar-remind-text{
	width: 52px;
	background: #FFFFFF;
	border-radius: 3px;
	text-align: center;
	color: #5c5c5c;
}
.ambuf-console-topbar .topbar-remind .topbar-remind-num{
	position: absolute;
	top: -12px;
	right: -5px;
	width: 30px;
	height: 30px;
	background-image: url(images/icons.png);
	background-repeat: no-repeat;
	text-align: center;
	color: #FFFFFF;
}

.ambuf-console-topbar .topbar-user-box{
	position: relative;
	width: 120px;
	height: 40px;
	line-height: 40px;
	margin-left: 10px;
	margin-top: 5px;
	background: #FFFFFF;
	border-radius: 3px;

}
.ambuf-console-topbar .topbar-user-name{
	width: 105px;
	text-align: center;
	overflow: hidden;
	white-space: nowrap;
	-o-text-overflow: ellipsis;
	text-overflow: ellipsis;
	word-break:keep-all;
}
.ambuf-console-topbar .topbar-user-arrow{
	position: absolute;
	top: 6px;
	right: 0;
	display: block;
	width: 30px;
	height: 30px;
	transition: transform 0.2s ease 0s, vertical-align 0.2s ease 0s;
}
.ambuf-console-topbar .topbar-user-logout{
	position: absolute;
	top: 35px;
	left: 0;
	width: 120px;
	background: #FFFFFF;
	border-radius: 3px;
	padding: 5px 0;
	display: none;
	text-align: center;
}
.ambuf-console-topbar .topbar-user-logout-text{
	display: inline-block;
	width: 90%;
	height: 30px;
	line-height: 30px;
	border-top: 1px solid #a4aaba;
	color: #5c5c5c;
}
.topbar-user-showLogout .topbar-user-logout-text{
	border-top: 1px solid #ffffff;
}
.topbar-user-showLogout .topbar-user-box
,.topbar-user-showLogout .topbar-user-logout{
	background: #ffffff;
}
.topbar-user-showLogout .topbar-user-logout-text{
	color: #addff2;
}
.topbar-user-showLogout .topbar-user-name{
	color: #FFFFFF;
}
.topbar-user-showLogout .topbar-user-arrow{
	transform: rotate(180deg);
}
.topbar-user-showLogout .topbar-user-logout{
	display: block;
}


.ambuf-console-topbar .topbar-user-logout-text:hover{
	color: #FFFFFF;
}
.ambuf-console-topbar .topbar-mobile-edition{
	border-left: 1px solid #ffffff;
}


/*
 *
 *  @module: 主界面左侧菜单 by zyc
 *  @content: start
